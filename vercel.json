{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/my-portfolio" }
      }
    ],
    "routes": [
      { "src": "/robots\\.txt", "dest": "/robots.txt" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
}
  